{
  "name": "voodoo773/app-user",
  "description": "User Module with Email and Social registration for ZF2",

  "repositories": [
  	{
      "type": "vcs",
      "url": "https://github.com/ksdevelopment/GoalioForgotPassword"
    }
    
  ],
  "require": {
      "php": ">=5.5",
      "zendframework/zendframework": "2.4.*",
      "zf-commons/zfc-base": "v0.1.2",
      "socalnick/scn-social-auth": "1.19.*",
      "hrevert/ht-user-registration": "0.0.*",
      "goalio/goalio-rememberme": "1.0.*",
      "goalio/goalio-forgotpassword": "dev-feature-email-format-option"
     
    
  },
  "license": "BSD-3-Clause",
  "keywords": [
    "framework",
    "zf2"
  ],
  "homepage": "https://github.com/Voodoo773",
  "autoload": {
    "psr-0": {
      "Hybrid": "vendor/hybridauth/hybridauth/hybridauth/"
    },
    "classmap": [
      "./vendor/hybridauth/hybridauth/additional-providers"
    ]
  }
}
